{
    "name": "seu-projeto/anamnese",
    "description": "Sistema de anamnese para cl√≠nicas",
    "type": "project",
    "require": {
        "php": "^8.0",
        "dompdf/dompdf": "^2.0",
        "illuminate/database": "^11.0",
        "laravel/serializable-closure": "^2.0",
        "monolog/monolog": "^3.0",
        "phpmailer/phpmailer": "^6.9",
        "tecnickcom/tcpdf": "*",
        "vlucas/phpdotenv": "^5.5",
        "wixel/gump": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "application/",
            "App\\": "application/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "vendor/bin/phpcs --standard=PSR12 application/",
        "cs-fix": "vendor/bin/phpcbf --standard=PSR12 application/",
        "install-pdf": [
            "composer require tecnickcom/tcpdf",
            "composer require dompdf/dompdf"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
